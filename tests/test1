3E 42 06 24 4F 21 00 C0 77 23 70 23 80 77 18 FE

3E 42    ; ld a, $42
06 24    ; ld b, $24
4F       ; ld c, a
21 00 C0 ; ld hl, $C000
77       ; ld [hl], a
23       ; inc hl
70       ; ld [hl], b
23       ; inc hl
80       ; add a, b
77       ; ld [hl], a
18 FE    ; jr Loop (jr $-2, infinite loop)


